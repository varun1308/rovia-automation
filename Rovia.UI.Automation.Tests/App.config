<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="LogManager"
             type="Rovia.UI.Automation.Logger.LogManagerConfigSection, Rovia.UI.Automation.Logger" />
    <section name="microsoft.visualstudio.testtools"
             type="Microsoft.VisualStudio.TestTools.UnitTesting.TestConfigurationSection, Microsoft.VisualStudio.QualityTools.UnitTestFramework, Version=10.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>
  </configSections>
  <connectionStrings>
    <add name="AirScenariosDataConnection" connectionString= "Driver={Microsoft Excel Driver (*.xlsx)};Dsn=Excel Files;dbq=|DataDirectory|\DataSheet\AirScenarios.xlsx;defaultdir=.; driverid=790;maxbuffersize=2048;pagetimeout=5" providerName="System.Data.Odbc" />
  </connectionStrings>

  <microsoft.visualstudio.testtools>
    <dataSources>
      <add name="AirLoginCreditCard" connectionString="AirScenariosDataConnection" dataTableName="AirLoginCreditCard$" dataAccessMethod="Sequential"/>
      <add name="AirGuestCreditCard" connectionString="AirScenariosDataConnection" dataTableName="AirGuestCreditCard$" dataAccessMethod="Sequential"/>
      <add name="AirLoginRoviaBucks" connectionString="AirScenariosDataConnection" dataTableName="AirLoginRoviaBucks$" dataAccessMethod="Sequential"/>

      <!--<add name="AirDataSource" connectionString="WebApiDataConnection" dataTableName="AirData$" dataAccessMethod="Sequential"/>
      <add name="CarDataSource" connectionString="WebApiDataConnection" dataTableName="CarData$" dataAccessMethod="Sequential"/>
      <add name="HotelDataSource" connectionString="WebApiDataConnection" dataTableName="HotelData$" dataAccessMethod="Sequential"/>-->
    </dataSources>
  </microsoft.visualstudio.testtools>


  <LogManager>
    <SummaryLoggers>
      <add alias ="fileLogger" namespace="Rovia.UI.Automation.Logger.FileLogger" assembly="Rovia.UI.Automation.Logger" target="SummaryLog"/>
    </SummaryLoggers>
    <DetailLoggers>
      <add alias ="fileLogger" namespace="Rovia.UI.Automation.Logger.FileLogger" assembly="Rovia.UI.Automation.Logger" target="DetailLog"/>
      <add alias ="consoleLogger" namespace="Rovia.UI.Automation.Logger.ConsoleLogger" assembly="Rovia.UI.Automation.Logger" target="Console"/>
    </DetailLoggers>
    <Severity value="All"/>

  </LogManager>
  <appSettings>
    <add key="driver" value="chrome" />
    <!--<add key="application.url" value="http://corp.rovia.com/"/>   Production-->
    <add key="application.url" value="http://dtl.qa-rovia.com/rovia" />
    <!--<add key="application.url" value="http://dtl.qa-rovia.com/rovia" />-->
    <add key="application.webuser.username" value="3285301" />
    <add key="application.webuser.password" value="test" />
    <add key="application.maxSearchDepth" value="5"/>
    <add key="application.summaryLogPath" value="SummaryLog"/>
    <add key="application.detailLogPath" value="DetailLog"/>
    <add key="application.severityLevel" value="All"/>
    <!--<add key="application.summaryLogPath" value="SummaryLog"/>-->
    
  </appSettings>
</configuration>
